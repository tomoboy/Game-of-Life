{
  "extends":  ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "no-submodule-imports":false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "allowSyntheticDefaultImports": true,
    "no-debugger": false,
    "ordered-imports": false
  },
  "jsRules": {
    "no-submodule-imports":false,
    "no-console": false,
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
